proc SECURE_POWER4:
    inputs : w8 a[0:3]
    outputs : w8 c[0:3];
    c[0] := a[0] &w8 a[0];
    c[0] := c[0] &w8 a[0];
    c[0] := c[0] &w8 a[0];
    c[1] := a[1] &w8 a[1];
    c[1] := c[1] &w8 a[1];
    c[1] := c[1] &w8 a[1];
    c[2] := a[2] &w8 a[2];
    c[2] := c[2] &w8 a[2];
    c[2] := c[2] &w8 a[2];
    c[3] := a[3] &w8 a[3];
    c[3] := c[3] &w8 a[3];
    c[3] := c[3] &w8 a[3];
end

verbose 1
noglitch order 3 Probing SECURE_POWER4